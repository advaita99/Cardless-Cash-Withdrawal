<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cardless"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.cardless.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.cardless.Register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="com.example.cardless.Request_for_verification"
            android:label="@string/title_activity_request_for_verification" >
        </activity>
        <activity
            android:name="com.example.cardless.View_acc_number"
            android:label="@string/title_activity_view_acc_number" >
        </activity>
        <activity
            android:name="com.example.cardless.Set_pin"
            android:label="@string/title_activity_set_pin" >
        </activity>
        <activity
            android:name="com.example.cardless.View_acc_balance"
            android:label="@string/title_activity_view_acc_balance" >
        </activity>
        <activity
            android:name="com.example.cardless.View_atm_transactions"
            android:label="@string/title_activity_view_atm_transactions" >
        </activity>
        <activity
            android:name="com.example.cardless.View_nearby_atm"
            android:label="@string/title_activity_view_nearby_atm" >
        </activity>
        <activity
            android:name="com.example.cardless.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="com.example.cardless.Post_complaint_reply"
            android:label="@string/title_activity_post_complaint_reply" >
        </activity>
        <activity
            android:name="com.example.cardless.Post_feedback"
            android:label="@string/title_activity_post_feedback" >
        </activity>
        <activity
            android:name="com.example.cardless.Homepage"
            android:label="@string/title_activity_homepage" >
        </activity>
        <activity
            android:name="com.example.cardless.View_bank"
            android:label="@string/title_activity_view_bank" >
        </activity>
        <activity
            android:name="com.example.cardless.View_my_account"
            android:label="@string/title_activity_view_my_account" >
        </activity>
        <activity
            android:name="com.example.cardless.Scan_QR"
            android:label="@string/title_activity_scan__qr" >
        </activity>
        <activity
            android:name="com.example.cardless.Rating"
            android:label="@string/title_activity_rating" >
        </activity>
    </application>

</manifest>
